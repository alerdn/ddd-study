{
  "extends": ["plugin:vitest-globals/recommended", "prettier"],
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-useless-constructor": "off",
    "prettier/prettier": "error"
  },
  "env": {
    "vitest-globals/env": true
  }
}
